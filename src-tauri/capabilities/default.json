{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "opener:default",
    "fs:default",
    "fs:allow-read-file",
    "fs:allow-write-file",
    "dialog:default",
    "dialog:allow-open",
    "dialog:allow-save",
    "deep-link:default",
    "deep-link:allow-get-current",
    "store:default"
  ],
  "platforms": ["linux", "macOS", "windows", "android", "iOS"]
}
